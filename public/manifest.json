{
  "short_name": "SocMedAnalyze",
  "name": "Social Analyze",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Keep track of your day-to-day social media usage!",
  "content_security_policy": "script-src 'self' 'sha256-Z8TvslM3FtoTGmNUMr3rzhB/7Sm+zWPe89dWKYUWEiU='; object-src 'self'",
  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "page_action": {
    "default_icon": "favicon.ico",
    "default_title": "SocMedAnalyze"
  },
  "permissions": ["storage"],
  "web_accessible_resources": ["index.html"]
}
